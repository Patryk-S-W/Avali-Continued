  <!-- ===== Hatched Pawns Stories ===== -->

  <AlienRace.BackstoryDef>
    <defName>AvaliFemaleRimworldChild</defName>
    <title>Rimworld kit female</title>
    <titleShort>Rimworld</titleShort>
    <baseDescription>[PAWN_nameDef] was hatched in one of colonies on rimworld planet. All [PAWN_possessive] childhood [PAWN_pronoun] helps other colonists to survive on this hostle place.</baseDescription>
    <slot>Childhood</slot>
    <bodyTypeGlobal>Thin</bodyTypeGlobal>
    <shuffleable>false</shuffleable>
    <spawnCategories>
      <li>AvaliBackstory</li>
    </spawnCategories>
    <forcedHediffs>
    </forcedHediffs>
  </AlienRace.BackstoryDef>

  <AlienRace.BackstoryDef>
    <defName>AvaliFemaleRimworldAdult</defName>
    <title>Rimworld colonist female</title>
    <titleShort>Rimworld</titleShort>
    <baseDescription>After [PAWN_nameDef] grew up [PAWN_pronoun] became a full member of the colony.</baseDescription>
    <slot>Adulthood</slot>
    <bodyTypeGlobal>Thin</bodyTypeGlobal>
    <shuffleable>false</shuffleable>
    <spawnCategories>
      <li>AvaliBackstory</li>
    </spawnCategories>
