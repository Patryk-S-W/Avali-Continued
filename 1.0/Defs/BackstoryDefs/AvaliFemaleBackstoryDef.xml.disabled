  <!-- ===== Child Stories ===== -->
   
  <!-- AvaliFemaleAvaliIlluminateChild -->
  <ThingDef Name="AvaliFemaleAvaliIlluminateChild" Abstract="True">
    <title>Illuminate child female</title>
    <titleShort>Illuminate</titleShort>
    <baseDescription>[PAWN_nameDef] was hatched in one of the training facilities devoted to producing the intelligent Illuminate Elite. [PAWN_possessive] formative years were occupied with many of the usual mundane types of education, but augmented with courses on observation and management.</baseDescription>
    <slot>Childhood</slot>
    <bodyTypeGlobal>Thin</bodyTypeGlobal>
    <maleCommonality>0</maleCommonality>
    <femaleCommonality>100</femaleCommonality>
    <spawnCategories>
      <li>AvaliBackstory</li>
    </spawnCategories>
    <skillGains>
      <li>
        <defName>Social</defName>
        <amount>6</amount>
      </li>
      <li>
        <defName>Medicine</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Intellectual</defName>
        <amount>2</amount>
      </li>
    </skillGains>
    <forcedHediffs>
      <li>GuardianNanites</li>
    </forcedHediffs>
  </ThingDef>
   
  <AlienRace.BackstoryDef ParentName="AvaliFemaleAvaliIlluminateChild">
    <defName>AvaliFemaleAvaliIlluminateChild</defName>
  </AlienRace.BackstoryDef>

  <!-- AvaliFemaleWarTornSurvivor -->
  <ThingDef Name="AvaliFemaleWarTornSurvivor" Abstract="True">
    <title>War-torn survivor female</title>
    <titleShort>War survivor</titleShort>
    <baseDescription>[PAWN_nameDef] was hatched on a war-torn world were food was rare and were other Avali had to fight for food. [PAWN_pronoun] never talks much and trys to avoid others in converstion because of their past life [PAWN_pronoun] tired to leave it behind. </baseDescription>
    <slot>Childhood</slot>
    <bodyTypeGlobal>Thin</bodyTypeGlobal>
    <maleCommonality>0</maleCommonality>
    <femaleCommonality>100</femaleCommonality>
    <spawnCategories>
      <li>AvaliBackstory</li>
    </spawnCategories>
    <skillGains>
      <li>
        <defName>Social</defName>
        <amount>-10</amount>
      </li>
      <li>
        <defName>Medicine</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Shooting</defName>
        <amount>8</amount>
      </li>
      <li>
        <defName>Melee</defName>
        <amount>6</amount>
      </li>
    </skillGains>
    <!-- <workDisables>
      <li>Caring</li>
    </workDisables> -->
    <forcedHediffs>
    </forcedHediffs>
  </ThingDef>
   
